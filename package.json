{
  "name": "@meshsdk/starter-template",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "compact": "turbo run compact",
    "build": "turbo run build",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "dev:proof-server": "pnpm --filter counter-cli run run-proof-server-testnet",
    "dev:undeployed-instances": "pnpm --filter counter-cli run prepare-standalone",
    "dev:frontend": "pnpm --filter frontend-vite-react run dev",
    "start-app-testnet": "concurrently \"pnpm run dev:proof-server\" \"pnpm run dev:frontend\"",
    "start-app-undeployed": "pnpm run dev:frontend",
    "format": "pnpm exec biome format --write ."
  },
  "engines": {
    "node": ">=18",
    "pnpm": ">=9"
  },
  "packageManager": "pnpm@9.12.3",
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@types/node": "^22.15.29",
    "@types/ws": "^8.18.1",
    "@biomejs/biome": "^1.9.4",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "concurrently": "^9.2.0",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "testcontainers": "^11.0.0",
    "ts-node": "^10.9.2",
    "turbo": "^2.4.4",
    "typescript": "^5.8.3",
    "vitest": "^3.2.0"
  },
  "dependencies": {
    "@meshsdk/midnight-react": "^0.0.6",
    "@midnight-ntwrk/compact-runtime": "0.8.1",
    "@midnight-ntwrk/dapp-connector-api": "3.0.0",
    "@midnight-ntwrk/ledger": "4.0.0",
    "@midnight-ntwrk/midnight-js-contracts": "2.0.2",
    "@midnight-ntwrk/midnight-js-fetch-zk-config-provider": "2.0.2",
    "@midnight-ntwrk/midnight-js-http-client-proof-provider": "2.0.2",
    "@midnight-ntwrk/midnight-js-indexer-public-data-provider": "2.0.2",
    "@midnight-ntwrk/midnight-js-level-private-state-provider": "2.0.2",
    "@midnight-ntwrk/midnight-js-node-zk-config-provider": "2.0.2",
    "@midnight-ntwrk/midnight-js-types": "2.0.2",
    "@midnight-ntwrk/wallet": "5.0.0",
    "@midnight-ntwrk/wallet-api": "5.0.0",
    "@midnight-ntwrk/zswap": "4.0.0",
    "pino": "9.7.0",
    "pino-pretty": "13.0.0",
    "vite-plugin-node-polyfills": "^0.24.0",
    "ws": "8.18.2"
  }
}
